INGRESS_INTF = swp+

INGRESS_CHAIN = INPUT

INNFWD_CHAIN = INPUT,FORWARD

SNMP_HOSTS = "172.18.236.81/32,172.31.31.175/32,172.23.30.147/32,10.33.12.24/32,10.33.12.96/32,172.23.16.46/32,172.27.16.6/32,172.27.16.5/32,172.31.16.7/32,172.31.16.9/32,172.31.16.8/32,10.247.6.25/32,172.27.16.91/32"

[iptables]

-A $INGRESS_CHAIN  -p tcp --dport 161 -s $SNMP_HOSTS -j ACCEPT
-A $INGRESS_CHAIN  -p tcp --dport 161 -j DROP
-A $INGRESS_CHAIN  -p udp --dport 161 -s $SNMP_HOSTS -j ACCEPT
-A $INGRESS_CHAIN  -p udp --dport 161 -j DROP
